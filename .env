# Frontend Development Environment Variables
NODE_ENV=development

# API Configuration
API_URL=http://localhost:5000/api/v1

# App Configuration
APP_NAME="Income & Expenditure System"
APP_VERSION=1.0.0
APP_ENVIRONMENT=development

# Feature Flags
ENABLE_LOGGING=true
ENABLE_ERROR_REPORTING=false
ENABLE_ANALYTICS=false

# Build Configuration
BUNDLE_OPTIMIZATION=false
MINIFY_BUNDLE=false
TREE_SHAKING=false

# Security
ENABLE_HTTPS=false

# Performance
ENABLE_CACHING=true
CACHE_EXPIRY=3600000

# Development Settings
EXPO_DEBUG=true
REACT_NATIVE_DEBUG=true