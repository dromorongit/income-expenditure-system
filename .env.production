# Frontend Production Environment Variables
NODE_ENV=production
EXPO_WEB_BABEL_ENV=production

# API Configuration
API_URL=https://myincomeexpendituresystem.netlify.app/api

# App Configuration
APP_NAME="Income & Expenditure System"
APP_VERSION=1.0.0
APP_ENVIRONMENT=production

# Feature Flags
ENABLE_LOGGING=false
ENABLE_ERROR_REPORTING=true
ENABLE_ANALYTICS=false

# Build Configuration
BUNDLE_OPTIMIZATION=true
MINIFY_BUNDLE=true
TREE_SHAKING=true

# Security
ENABLE_HTTPS=true

# Performance
ENABLE_CACHING=true
CACHE_EXPIRY=3600000

# Expo Web Configuration
EXPO_WEB_OUTPUT_DIR=dist
EXPO_WEB_VIEW_DIR=web-build