# Frontend Production Environment Variables
NODE_ENV=production

# API Configuration
API_URL=https://income-expenditure-system-production.up.railway.app/api

# App Configuration
APP_NAME="Income & Expenditure System"
APP_VERSION=1.0.0
APP_ENVIRONMENT=production

# Feature Flags
ENABLE_LOGGING=false
ENABLE_ERROR_REPORTING=true
ENABLE_ANALYTICS=true

# Build Configuration
BUNDLE_OPTIMIZATION=true
MINIFY_BUNDLE=true
TREE_SHAKING=true

# Security
ENABLE_HTTPS=true
SSL_CERT_PATH=./ssl/cert.pem

# Performance
ENABLE_CACHING=true
CACHE_EXPIRY=3600000

# Monitoring
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
ANALYTICS_ID=your-google-analytics-id